@model List<ELearningProject.DAL.Entities.CourseRegister>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/InstructorAdminLayout/Index.cshtml";
    int count = 0;
}

<h2>Index</h2>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Ad Soyad</th>
        <th>Mail</th>
        <th>Kurs Adı</th>
    </tr>
    @foreach (var item in Model)
    {
        count++;
        <tr>
            <td>@count</td>
            <td>@item.Student.Name @item.Student.Surname</td>
            <td>@item.Student.Mail</td>
            <td>@item.Course.Title</td> 
        </tr>
    }
</table>

